QUERY:

query NodeQuery(
  $id: ID!
) {
  node(id: $id) {
    ... on Story {
      comments(first: 10) {
        edges {
          ... on CommentsEdge {
            node {
              __typename
              id
            }
            cursor
          }
        }
        pageInfo {
          endCursor
          hasNextPage
        }
      }
    }
    __typename
    id
  }
}
