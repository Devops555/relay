        edges {
          node {
            id
          }
        }
      }
      mixedDirectionComments: comments(last: $last, first: $first)
        @connection(key: "NodeQuery_mixedDirectionComments") {
        edges {
          node {
            id
          }
        }
      }
    }
  }
}
