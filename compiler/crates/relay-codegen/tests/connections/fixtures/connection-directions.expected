query NodeQuery($id: ID!, $last: Int, $first: Int) {
  node(id: $id) {
    id
    ... on Story {
      forwardComments: comments(first: 1)
        @connection(key: "NodeQuery_forwardComments") {
        edges {
          node {
            id
          }
        }
      }
      backwardComments: comments(last: 2)
